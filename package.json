{
  "name": "new-demo-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --force-exit --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "npm run test -- --collectCoverage --ci",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "npx typeorm -d dist/ormconfig.js",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:create": "typeorm migration:create",
    "up": "npm run typeorm -- migration:run",
    "down": "npm run typeorm  -- migration:revert",
    "dispatch-messages": "node dist/src/infrastructure/message-bus/cli-commands/command-handler dispatch-messages",
    "handle-messages": "node dist/src/infrastructure/message-bus/cli-commands/command-handler handle-messages"
  },
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "@nestjs/common": "^10.4.8",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.6",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@slynova/flydrive-gcs": "^1.0.3",
    "amqplib": "^0.10.4",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "http-problem-details": "^0.1.5",
    "http-problem-details-mapper": "^0.1.7",
    "install": "^0.13.0",
    "jest-junit": "^16.0.0",
    "jest-sonar": "^0.2.16",
    "jsonrepair": "^3.10.0",
    "nest-commander": "^3.15.0",
    "nodemailer": "^7.0.3",
    "npm": "^10.9.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.6.3",
    "typeorm-transactional": "^0.5.0",
    "typescript-eslint": "^8.31.0",
    "uid": "^2.0.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.handler.ts",
      "src/**/*.controller.ts",
      "src/**/*.repository.ts",
      "src/**/*.entity.ts",
      "!src/**/*.enum.ts",
      "!src/**/processors/**/*",
      "!src/**/message-bus/**/*",
      "!src/**/*.module.ts",
      "!src/domains/**/events/**/*",
      "!src/infrastructure/message-bus/rabbitmq/config/*.ts",
      "!src/infrastructure/http/**/*",
      "!src/infrastructure/database/**/*"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^test/(.*)$": "<rootDir>/test/$1",
      "ormconfig": "<rootDir>/ormconfig.ts"
    },
    "coverageReporters": [
      "cobertura",
      "clover",
      "lcov",
      "text"
    ],
    "reporters": [
      "default",
      "jest-junit",
      [
        "jest-sonar",
        {
          "outputName": "test-report.xml",
          "reportedFilePath": "relative",
          "relativeRootDir": "."
        }
      ]
    ]
  },
  "jestSonar": {
    "reportPath": "./coverage"
  },
  "jest-junit": {
    "outputDirectory": "./coverage/",
    "outputName": "junit.xml"
  }
}
